package testprj_01;

public class hello {
	public static void main(String[] args) {
		hello hy = new total(1,2);
		hy.print();
	}
}
public class total{
	private int val1 = 0;
	private static int val2 =0;

	public total(int val1, int val2){
		this.val1 += val1;
		total.val2 += val2;
	}
	public void print(){
		System.out.println(val1+","+val2)
	}
}
