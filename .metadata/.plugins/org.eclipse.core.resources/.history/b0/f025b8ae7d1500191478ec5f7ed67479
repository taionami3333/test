package testprj_01;

public class hello {
	public static void main(String[] args){
		System.out.println("Main method");
		sub();
 	}

	public static void second(){
		System.out.println("Second method");
	}

	public static void third(){
		System.out.println("Third method");
		for(int i=0; i<3; i++){
			second();
		}
	}
 }